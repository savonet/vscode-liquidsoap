{
  "comments": {
    "lineComment": "#",
    "blockComment": ["#<", ">#"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "indentationRules": {
    "decreaseIndentPattern": "\\b(end|elsif|else)\\b",
    "increaseIndentPattern": "\\b(begin|let|def|for|while)\\b"
  },
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["r/", "/"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    ["r/", "/"]
  ]
}
